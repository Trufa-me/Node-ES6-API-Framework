  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch App.js",
      "program": "${workspaceRoot}/src/index.js",
      "outFiles": [
        "${workspaceRoot}/compiled/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Current File",
      "program": "${file}",
      "outFiles": [ "${workspaceRoot}/compiled/**/*.js" ]
    },
    {
      "name": "Launch Remote App",
      "type": "node",
      "protocol": "inspector",
      "request": "attach",
      "stopOnEntry": false,
      "port": 10000,
      "address": "192.168.10.10",
      "localRoot": "${workspaceRoot}",
      "remoteRoot": "/Users/mokrzk01/ES6-API-Framework",
      "sourceMaps": true
    }
  ]
}
