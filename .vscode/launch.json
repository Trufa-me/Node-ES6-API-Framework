  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch App.js",
      "program": "${workspaceRoot}/src/index.js",
      "outFiles": [
        "${workspaceRoot}/compiled/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Current File",
      "program": "${file}",
      "outFiles": [ "${workspaceRoot}/compiled/**/*.js" ]
    },
    {
      "name": "Attache Remote App.js",
      "type": "node",
      "request": "attach",
      "port": 10000,
      "address": "192.168.10.10"
    },
  ]
}
