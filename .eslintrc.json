{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "mocha": true
  },
  "root": true,
  "rules": {
    "no-underscore-dangle": 0,
    "no-useless-constructor": 0,
    "arrow-parens": 0,
    "import/no-extraneous-dependencies": 0,
    "global-require": 0,
    "radix": 0,
    "no-param-reassign": 0
  },
  "plugins": [
      "import"
  ]
}
